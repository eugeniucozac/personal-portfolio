<header>
	<div class="social-left clearfix">
		<h1><a><img src="/images/logo.svg" alt="logo"></a></h1>
		<ul>
			<li>
				<a href="<%= linkedin %>" target="_blank"><i class="fab fa-linkedin-in"></i></a>
			</li>
			<li>
				<a href="<%= github %>" target="_blank"><i class="fab fa-github"></i></a>
			</li>
			<li>
				<a><i class="fab fa-twitter"></i></a>
			</li>
		</ul>
	</div>
	<section class="container">
		<div class="row">
			<div class="col-sm-12">
				<nav class="d-md-block d-none">
					<ul class="nav">
						<li class="nav-item">
						    <a class="nav-link active px-md-3" data-click="home">Home</a>
						</li>
						<li class="nav-item">
						    <a class="nav-link px-md-3" data-click="about">About</a>
						</li>
						<li class="nav-item">
						    <a class="nav-link px-md-3" data-click="experience">Experience</a>
						</li>
						<li class="nav-item">
						    <a class="nav-link px-md-3" data-click="skills">Skills</a>
						</li>
						<li class="nav-item">
						    <a class="nav-link px-md-3" data-click="projects">Portfolio</a>
						</li>
						<li class="nav-item">
						    <a class="nav-link px-md-3" data-click="contact">Contact</a>
						</li>
					</ul>
				</nav>
				<section>
					<% headers.forEach((item) => { %>
						<div class="row">	
						  <div class="col-md-6 col-sm-12">
						  	  <section class="jumbotron mb-0 pb-sm-0-down">
						  	  	<div>Hi, there</div>
						  	  	<h3>Iâ€™m <%- item.name %></h3>
							  	<p><%= item.experience %></p>
							  	<h4><%= item.position %></h4>
							  	<a class="btn btn-primary btn-lg contact-button" data-click="contact" role="button">contact me</a>
							  </section>
						  </div>
						  <div class="col-md-6 d-md-block d-none">
						  	<img src="<%= item.image %>" class="card-img" alt="<%= item.image %>">
						  </div>
						</div>	
					<% }) %>
				</section>
			</div>
		</div>	
	</section>
</header>